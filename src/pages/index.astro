---
import Accessories from "../components/Accesories.astro";
import FirstSection from "../components/FirstSection.astro";
import Landingheader from "../components/Landingheader.astro";
import ModelS from "../components/ModelS.astro";
import Model3 from "../components/Model3.astro";
import ModelX from "../components/ModelX.astro";
import ModelY from "../components/ModelY.astro";
import PowerWall from "../components/PowerWall.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout title="Tesla Duplicated">
  <Landingheader />

  <main
    class="snap-y snap-mandatory relative w-full h-screen overflow-auto overflow-x-hidden"
  >
    <div class="snap-center">
      <ModelY />
    </div>

    <div class="snap-center">
      <Model3 />
    </div>

    <div class="snap-center">
      <ModelS />
    </div>

    <div class="snap-center">
      <ModelX />
    </div>
    <div class="snap-center">
      <FirstSection />
    </div>

    <div class="snap-center">
      <PowerWall />
    </div>

    <div class="snap-center">
      <Accessories />
    </div>
  </main>
</Layout>
